{
  "npmScope": "juge",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "experiment-one": {
      "tags": ["scope:juge"]
    },
    "sandbox": {
      "tags": ["scope:juge"]
    },
    "sandbox-e2e": {
      "tags": ["scope:juge"],
      "implicitDependencies": ["sandbox"]
    },
    "shell": {
      "tags": ["scope:juge"]
    },
    "ui-table": {
      "tags": ["scope:juge"]
    }
  }
}
